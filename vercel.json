{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Private-Network",
          "value": "true"
        },
        {
          "key": "Access-Control-Request-Private-Network",
          "value": "false"
        }
      ]
    }
  ]
}
